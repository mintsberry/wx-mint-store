@import "../../wxss/common.wxss";

.container{
  display:flex;
  flex-direction:column;
  align-items: center;
  padding-top:40rpx;
  background-color: #fff;
}

.title {
  width: 690rpx;
  height: 90rpx;
}

.box {
  display: flex;
  margin-top: 30rpx;
  justify-content: space-between;
  width: 100%;
}

.left {
  height: 522rpx;
}

.left > .img {
  width: 346rpx;
  height: 522rpx;
}

.right {
  display: flex;
  flex-direction: column;
  justify-content: space-between;
}

.right >.img {
  width: 400rpx;
  height: 260rpx;
}